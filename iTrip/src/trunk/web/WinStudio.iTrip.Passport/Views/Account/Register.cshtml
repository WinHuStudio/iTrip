@model WinStudio.iTrip.Passport.Models.RegisterModel

@{
    ViewBag.Title = "Register";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<form class="form-horizontal" action="/Account/Register" method="post">
    <div class="control-group">
        <label class="control-label" for="inputAccount">Account</label>
        <div class="controls">
            <input type="text" id="inputAccount" name="UserAccount" placeholder="Account">
        </div>
    </div>
    <div class="control-group">
        <label class="control-label" for="inputAccount">UserName</label>
        <div class="controls">
            <input type="text" id="inputName" name="UserName" placeholder="Name">
        </div>
    </div>
    <div class="control-group">
        <label class="control-label" for="inputPassword">Password</label>
        <div class="controls">
            <input type="password" id="inputPassword" name="Password" placeholder="Password">
        </div>
    </div>
    <div class="control-group">
        <label class="control-label" for="inputConfirmPassword">ConfirmPassword</label>
        <div class="controls">
            <input type="password" id="inputConfirmPassword" name="ConfirmPassword" placeholder="ConfirmPassword">
        </div>
    </div>
    <div class="control-group">
        <label class="control-label" for="inputEmail">Email</label>
        <div class="controls">
            <input type="text" id="inputEmail" name="Email" placeholder="Email">
        </div>
    </div>
    <div class="control-group">
        <div class="controls">
            <button type="submit" class="btn">Sign in</button>
        </div>
    </div>
</form>

